package Atividades_Exercicios;

import java.util.Scanner;

public class Matrizes_Dois {

	public static void main(String[] args) {
		
	Scanner sc = new Scanner(System.in);	
	
	int n = sc.nextInt();
	int m = sc.nextInt();
	int[][] mat = new int [n][m];
	
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < m; j++) {
			mat[i][j] = sc.nextInt();
		}
	}
	int x = sc.nextInt();
	
	for (int l = 0; l < x; l++) {
		System.out.println("Position " + mat[x][x] + ":");
	}
	
	
	
	
	sc.close();
	}
}
